{
  "name": "Job Scanner",
  "description": "",
  "version": "0.1.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open App"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["./assets/content.js"]
    }
  ]
}
